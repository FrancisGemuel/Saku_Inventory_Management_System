/* QTableWidget items (cells) */
QTableWidget::item {
    border:1px solid black;
}

/* QTableWidget headers */
QHeaderView::section {
    border: 1px solid black;
    background: rgb(240, 240, 240); /* Optional: Header background color */
    padding: 4px;
}

/* Vertical scrollbar */
QScrollBar:vertical {
    border: 2px solid black;
    background: rgb(52, 58, 64); /* Scrollbar background color */
    width: 13px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #000000; /* Scrollbar handle color */
    border: 1px solid white;
    border-radius: 4px;
    min-height: 15px;
}

QScrollBar::add-line:vertical {
    background: #000000; /* Down button color */
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    background: #000000; /* Up button color */
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    border: 1px solid grey;
    width: 5px;
    height: 5px;
    background: white;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Horizontal scrollbar */
QScrollBar:horizontal {
    border: 2px solid black;
    background: rgb(52, 58, 64); /* Scrollbar background color */
    height: 13px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:horizontal {
    background: #000000; /* Scrollbar handle color */
    border: 1px solid white;
    border-radius: 4px;
    min-width: 20px;
}

QScrollBar::add-line:horizontal {
    background: #000000; /* Right button color */
    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    background: #000000; /* Left button color */
    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
    border: 1px solid grey;
    width: 3px;
    height: 3px;
    background: white;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

