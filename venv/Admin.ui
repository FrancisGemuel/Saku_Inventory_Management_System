QScrollBar:vertical {
    border: 2px solid black;
    background: rgb(52, 58, 64); /* Scrollbar background color */
    width: 13px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #000000; /* Scrollbar handle color */
    border: 1px solid white;
    border-radius: 4px;
    min-height: 15px;
}

QScrollBar::add-line:vertical {
    background: #000000; /* Down button color */
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    background: #000000; /* Up button color */
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

